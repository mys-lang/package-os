from os import cd
from os import cwd
from os import ls
from os import touch
from os import which
from os.path import Path

def main():
    if which("ls") is not None:
        print("executable ls found")

    Path("foo").mkdir()
    cd(Path("foo"))
    print("Current working directory is", cwd())
    touch("bar")

    if Path("bar").exists():
        print("bar exists")

    print(f"Files:", ls())
    cd(Path(".."))
    Path("foo").rm(recursive=True, force=True)
