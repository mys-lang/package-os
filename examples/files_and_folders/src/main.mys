from os import cd
from os import cwd
from os import exists
from os import ls
from os import mkdir
from os import rm
from os import touch
from os import which

def main():
    if which("ls") is not None:
        print("executable ls found")

    mkdir("foo")
    cd("foo")
    print("Current working directory is", cwd())
    touch("bar")

    if exists("bar"):
        print("bar exists")

    print(f"Files:", ls())
    cd("..")
    rm("foo", recursive=True, force=True)
